  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "scripts": {
    "start": "node build/bundle.js",
    "build": "rimraf public && webpack  --config webpack.server.js && webpack -p --config webpack.client.prod.js --mode production && node scripts/compile",
    "heroku-postbuild": "npm install && npm run build:prod",
    "webpack-prod": "rimraf public && webpack -p --progress --config webpack.client.prod.js --mode production",
    "dev": "npm run build-server && npm-run-all --parallel dev:*",
    "build-server": "rimraf build && webpack --config webpack.server.js",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "precommit": "pretty-quick --staged",
    "base": "webpack --config webpack.base.js --watch",
    "sitemap": "babel-node ./src/sitemap-generator.js"
  },
